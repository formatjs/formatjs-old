    "lodash": "^4.17.15",
    "loud-rejection": "^2.1.0"
  },
  "bugs": {
    "url": "https://github.com/formatjs/formatjs/issues"
  },
  "engines": {
    "node": "8.x || 10.x || 12.x"
  },
  "devDependencies": {
    "@types/rimraf": "^2.0.2",
    "rimraf": "^3.0.0"
