{
  "name": "@formatjs/intl-pluralrules",
  "version": "1.5.4",
  "description": "Polyfill for intl plural rules",
  "keywords": [
    "polyfill",
    "i18n",
    "intl",
    "plural",
    "rules"
  ],
  "author": "Long Ho <holevietlong@gmail.com>",
  "homepage": "https://github.com/formatjs/formatjs",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist-es6",
    "dist",
    "lib",
    "polyfill-locales.js",
    "polyfill.js",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formatjs/formatjs.git"
  },
  "devDependencies": {
    "formatjs-extract-cldr-data": "^10.1.3"
  },
  "dependencies": {
    "@formatjs/intl-utils": "^2.2.1"
  },
  "scripts": {
    "build:cjs": "npm run cldr && tsc -b tsconfig.cjs.json",
    "build": "npm run build:cjs && npm run build:additional",
    "build:additional": "tsc -p tsconfig.esm.json && tsc -p tsconfig.es6.json && rollup -c rollup.config.js && api-extractor run --local",
    "cldr": "ts-node --project tsconfig.cjs.json scripts/cldr",
    "clean": "rimraf dist lib dist-es6 polyfill-locales.js *.tsbuildinfo",
    "jest": "cross-env NODE_ICU_DATA=../../node_modules/full-icu TS_NODE_PROJECT=tsconfig.json cross-env NODE_ENV=test jest",
    "test262": "cross-env NODE_ICU_DATA=../../node_modules/full-icu TS_NODE_PROJECT=tsconfig.json ts-node tests/runner",
    "test": "npm run jest && npm run test262"
  },
  "bugs": {
    "url": "https://github.com/formatjs/formatjs/issues"
  },
  "gitHead": "a7842673d8ad205171ad7c8cb8bb2f318b427c0c"
}
