  "homepage": "https://github.com/yahoo/intl-format-cache",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^12.0.1",
    "expect": "^1.13.0",
    "grunt": "^0.4.5",
    "grunt-bundle-jsnext-lib": "^0.5.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-saucelabs": "^8.6.1",
    "intl": "^1.0.1",
